{
  "extends": "airbnb",
  "rules": {
    "no-useless-escape": 0,
    "brace-style": 0,
    "no-throw-literal": 0,
    "no-unused-vars": 1,
    "max-len": 1
    },

  "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".mjs"
          ]
        }
      }
    }
}
